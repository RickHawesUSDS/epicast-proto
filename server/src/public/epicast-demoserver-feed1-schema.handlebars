# version of the epicast that is used
epicastVersion: 0.1

# Subject ID represent of the reporting subject.
# For cases, the jurisdiction or the population that is the subject of the reporting.
# In this demo, epicast is used to represent jurisdition
subjectId: {{ subjectId }}

# The reporter is the organization, system or facility reporting on the subject.
# For cases, this will likely be a name of a public health agency
reporterId: {{ reporterId }}

# The topic of the feed. For cases, this will be something like 'covid_19_cases' 
topicId: {{ topicId }}

# The date that this schema was used in this feed
validFrom: {{ validFrom }}

#
# Elements of the schema
#
# Each element has:
# - name
# - displayName (optional)
# - description (optional)
# - type: either 'string', 'date', 'number', or 'code'
# - codeSet (optional)
# - valueSet (optional)
# - validation (optional)
#
elements:
{{#each elements}}
- name: {{ this.name }}
  {{#if this.displayName }}
  displayName: {{ this.displayName }}
  {{/if}}
  {{#if this.description }}
  description: {{this.description}}
  {{/if}}
  type: {{ this.type }}
  {{#if this.codeSet}}
  codeSet: {{this.codeSet}}
  {{/if}}
  {{#if this.valueSet}}
  valueSet: {{this.valueSet}}
  {{/if}}
  {{#if this.validation}}
  validation: {{this.validation}}
  {{/if}}
  tags:
  {{#each this.tags }}
  - {{ this }}
  {{/each}}
  memberOfSchemas:
  {{#each this.memberOfSchemas }}
  - {{ this }}
  {{/each}}
{{/each}}
